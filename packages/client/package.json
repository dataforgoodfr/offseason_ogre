{
  "name": "ogre-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.5.5",
    "@mui/icons-material": "^5.6.1",
    "@mui/lab": "^5.0.0-alpha.80",
    "@mui/material": "^5.6.1",
    "@mui/system": "^5.11.4",
    "@mui/x-data-grid": "^5.17.18",
    "@mui/x-date-pickers": "^5.0.0-alpha.3",
    "@sentry/react": "^7.43.0",
    "@sentry/tracing": "^7.43.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.11.21",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/recharts": "^1.8.24",
    "axios": "^0.25.0",
    "date-fns": "^2.28.0",
    "get-user-locale": "^2.2.1",
    "i18next": "^22.4.14",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.30.0",
    "react-hotjar": "^5.2.0",
    "react-i18next": "^12.2.0",
    "react-query": "^3.38.1",
    "react-router-dom": "^6.3.0",
    "recharts": "^2.4.1",
    "socket.io-client": "^4.6.0",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "yarn check-type && vite build",
    "serve": "vite preview",
    "check-type": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "yarn prettier --check .",
    "prettier:write": "yarn prettier --write .",
    "test": "vitest watch",
    "test:no-watch": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "no-use-before-define": "off",
      "no-restricted-imports": ["error", {
        "name": "axios",
        "message": "Please use `http`, the axios client configured in the request utils (src/utils/request)."
      }],
      "@typescript-eslint/no-use-before-define": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^0.4.0",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "jsdom": "^20.0.3",
    "postcss": "^8.4.20",
    "prettier": "2.6.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "tailwindcss": "^3.2.4",
    "vite": "^4.0.3",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.26.2"
  },
  "proxy": "http://localhost:8080"
}
