{
  "compilerOptions": {
    "outDir": "./built",
    "allowJs": true,
    "esModuleInterop": true,
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "typeRoots": ["@types", "node_modules/@types"],
    "strict": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
